    <style>                            
        .ribbon-wrap {
            position: relative;
        }
        .acc_style04 {
            background: #edbe00 ;
            padding: 10px 15px 10px 42px;
            color: #fff;
            font-weight: 500;
            transition: all 0.2s ease-in;
            position: relative;
        }
        .acc_style04:hover{
            color: #eee;
        }
        .acc_style04::before {
            content: "";
            position: absolute;
            left: 14px;
            top: -2px;
            width: 24px;
            height: 74%;            
            background: #f1012d;
            clip-path: polygon(100% 0%, 100% 100%, 50% 75%, 0 100%, 0 0);
            transition: all 0.2s ease-in;
        }
        .acc_style04:hover::before {
            height: 90%;            
        }
    </style>

    <style>      
        @@keyframes slideIn {
            from {
                transform: translateX(400px);
                opacity: 0;
            }

            to {
                transform: translateX(0);
                opacity: 1;
            }
        }

        @@keyframes pulse {
            0%, 100% {
                transform: scale(1);
            }

            50% {
                transform: scale(1.05);
            }
        }

        @@keyframes glow {
            0%, 100% {
                box-shadow: 0 4px 20px rgba(102, 126, 234, 0.4);
            }

            50% {
                box-shadow: 0 4px 30px rgba(102, 126, 234, 0.8), 0 0 40px rgba(118, 75, 162, 0.4);
            }
        }

        @@keyframes countUp {
            0% {
                transform: translateY(20px);
                opacity: 0;
            }

            50% {
                transform: translateY(-5px);
                opacity: 1;
            }

            100% {
                transform: translateY(0);
                opacity: 1;
            }
        }

        .timer-widget {
            position: fixed;
            bottom: 20px;
            right: 20px;
            background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
            color: white;
            padding: 22px 22px 18px;
            border-radius: 16px;
            box-shadow: 0 4px 20px rgba(0,0,0,0.3);
            min-width: 260px;
            z-index: 10000;
            font-family: 'Courier New', monospace;
            animation: slideIn 0.6s cubic-bezier(0.68, -0.55, 0.265, 1.55);
            backdrop-filter: blur(10px);
            /* overflow: hidden; */
        }

            .timer-widget.running {
                animation: glow 2s infinite;
            }

        .timer-display {
            font-size: 28px;
            font-weight: 800;
            text-align: center;
            margin: 8px 0 12px;
            letter-spacing: 2px;
            text-shadow: 0 2px 10px rgba(0,0,0,0.3);
            position: relative;
            line-height: 1.2;
            white-space: nowrap;
        }

            .timer-display.animate {
                animation: countUp 0.25s ease-out;
            }

        .timer-label {
            text-align: center;
            font-size: 12px;
            opacity: 0.85;
            margin-bottom: 6px;
            text-transform: uppercase;
            letter-spacing: 1px;
        }

        .timer-sub {
            text-align: center;
            font-size: 12px;
            opacity: 0.85;
            margin-top: 2px;
            letter-spacing: 0.5px;
            font-family: system-ui, -apple-system, 'Segoe UI', Roboto, sans-serif;
        }

        .progress-ring {
            position: absolute;
            top: -5px;
            left: -5px;
            right: -5px;
            bottom: -5px;
            border-radius: 16px;
            border: 3px solid rgba(255,255,255,0.25);
            animation: rotate 3s linear infinite;
            opacity: 0;
            transition: opacity 0.3s;
            pointer-events: none;
            overflow: hidden;
        }

        .timer-widget.running .progress-ring {
            opacity: 1;
        }

        @@keyframes rotate {
            from {
                transform: rotate(0deg);
            }

            to {
                transform: rotate(360deg);
            }
        }

        .timer-badge {
            display: inline-block;
            font-size: 11px;
            padding: 4px 10px;
            border-radius: 999px;
            background: rgba(255,255,255,0.18);
            border: 1px solid rgba(255,255,255,0.35);
            letter-spacing: 0.8px;
            text-transform: uppercase;
            font-family: system-ui, -apple-system, 'Segoe UI', Roboto, sans-serif;
        }

        .timer-head {
            display: flex;
            justify-content: center;
            margin-bottom: 6px;
        }

        .timer-expired .timer-badge {
            background: rgba(255,255,255,0.12);
        }

        .timer-expired .timer-display {
            opacity: 0.9;
        }
    </style>
    <style>
        /* Toggle button (clock icon) */
        .timer-toggle {
            position: absolute;
            top: -14px;
            right: -14px;
            width: 42px;
            height: 42px;
            border-radius: 50%;
            background: linear-gradient(135deg, #00d2ff, #3a7bd5);
            color: #fff;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 20px;
            cursor: pointer;
            box-shadow: 0 6px 18px rgba(0,0,0,0.35);
            transition: transform 0.25s ease, box-shadow 0.25s ease;
            z-index: 99999;
        }
        .timer-toggle:hover {
            transform: scale(1.1);
            box-shadow: 0 8px 24px rgba(0,0,0,0.45);
        }

        /* Minimized state */
        .timer-widget.minimized {
            width: 0;
            height: 0;
            padding: 0;
            border-radius: 50%;
            cursor: pointer;
        }

        .timer-widget.minimized *:not(.timer-toggle) {
            /* display: none; */
        }

        .timer-widget.minimized .timer-toggle {
        position: absolute;
        width: 42px;
        height: 42px;
        font-size: 24px;
        border-radius: 50%;
        right:10px;
        top: auto;
        bottom: 10px;}
</style>
</head>

